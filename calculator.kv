<Button>    size_hint: (0.2, 0.2)    background_normal: ''    #background_color: (2/255, 10/255, 24/255, 1)    background_color: (215/255, 215/255, 215/255, 1)<CalculatorWidget>#TextColor = 1, 1, 1, 1    BoxLayout        orientation: "vertical"        size: root.width, root.height        TextInput:            id: input_box            text: "0"            font_size: 43            multiline: False            halign: "right"            size_hint: (1, 0.19)            background_color: (20/255, 20/255, 30/255, 1)            foreground_color: (1, 1, 1, 1)        GridLayout:            cols: 4            rows: 5            ### Row 1 ####            Button:                text: "%"                color: 0, 0, 1, 1                font_size: 32                on_press: root.sings("%")            Button:                text: "C"                color: 1,0,0,1                font_size: 32                on_press: root.clear()            Button:                text: u"\u00AB"                color: 0, 0, 1, 1                font_size: 32                on_press: root.remove_last()            Button:                text: "/"                color: 0, 0, 1, 1                font_size: 32                #background_color: (14/255, 73/255, 176/255, 1)                on_press: root.sings("/")                Image:                    source: 'images/divide.png'            #### Row 2 #####            Button:                text: "7"                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.button_value(7)            Button:                text: "8"                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.button_value(8)            Button:                text: "9"                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.button_value(9)            Button:                text: "x"                color: 0, 0, 1, 1                font_size: 32                #background_color: (14/255, 73/255, 176/255, 1)                on_press: root.sings("*")            #### Row 3 ###            Button:                text: "4"                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.button_value(4)            Button:                text: "5"                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.button_value(5)            Button:                text: "6"                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.button_value(6)            Button:                text: "-"                color: 0, 0, 1, 1                font_size: 32                #background_color: (14/255, 73/255, 176/255, 1)                on_press: root.sings("-")            ### Row 4 ###            Button:                text: "1"                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.button_value(1)            Button:                text: "2"                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.button_value(2)            Button:                text: "3"                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.button_value(3)            Button:                text: "+"                color: 0, 0, 1, 1                font_size: 32                #background_color: (14/255, 73/255, 176/255, 1)                on_press: root.sings("+")            ### Row 5 ###            Button:                text: "+/-"                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.positive_negative()            Button:                text: "0"                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.button_value(0)            Button:                text: "."                color: 0, 0, 0, 0.3                font_size: 32                on_press: root.dot()            Button:                text: "="                color: 1, 1, 1, 1                font_size: 32                background_color: (14/255, 73/255, 176/255, 1)                on_press: root.results()